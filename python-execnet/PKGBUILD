pkgname=python-execnet
pkgver=1.1
pkgrel=1
pkgdesc="execnet: rapid multi-Python deployment"
arch=('any')
license=('GPL')
url="http://codespeak.net/execnet"
depends=('python')
makedepends=('python-distribute')
source=(http://pypi.python.org/packages/source/e/execnet/execnet-$pkgver.zip)
md5sums=('be885ccd9612966bb81839670d2da099')

build() {
  cd "${srcdir}/execnet-${pkgver}"
  python setup.py install --root="${pkgdir}" --optimize=1
}
